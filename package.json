{
  "name": "mimic-mania-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "prestart": "npm run build",
    "start:development": "cross-env NODE_ENV=development node dist/src/index.js",
    "start:production": "cross-env NODE_ENV=production node dist/src/index.js",
    "migrate-prisma": "npx prisma migrate dev --name init",
    "seed": "npx prisma db seed",
    "db-push": "npx prisma db push"
  },
  "keywords": [],
  "author": "Cesar Riva & Caio Riva",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "awilix": "^8.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "rimraf": "^5.0.0",
    "socket.io": "^4.6.2",
    "typescript": "^5.0.4",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.1.0",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1"
  }
}
